{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/config",
      "dest": "server.js"
    },
    {
      "src": "/create-checkout-session",
      "dest": "server.js"
    },
    {
      "src": "/session-status",
      "dest": "server.js"
    },
    {
      "src": "/coupons",
      "dest": "server.js"
    },
    {
      "src": "/create-test-coupon",
      "dest": "server.js"
    },
    {
      "src": "/create-payment-intent",
      "dest": "server.js"
    },
    {
      "src": "/payment-status",
      "dest": "server.js"
    },
    {
      "src": "/webhook",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"
    }
  ]
}
